{
  "name": "@lodado/sdui-template-component",
  "version": "1.0.0",
  "description": "SDUI Component Library - Radix UI-based components for Server-Driven UI",
  "author": "lodado",
  "license": "MIT",
  "packageManager": "pnpm@9.0.6",
  "main": "./dist/cjs/client/index.cjs",
  "module": "./dist/es/client/index.mjs",
  "types": "./dist/types/index.d.ts",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "import": {
          "default": "./dist/es/client/index.mjs",
          "types": "./dist/types/index.d.ts"
        },
        "require": {
          "default": "./dist/cjs/client/index.cjs",
          "types": "./dist/types/index.d.ts"
        }
      }
    },
    "main": "./dist/cjs/client/index.cjs",
    "module": "./dist/es/client/index.mjs",
    "source": "./src/index.ts",
    "types": "./dist/types/index.d.ts"
  },
  "scripts": {
    "build": "rollup -c && tsc --emitDeclarationOnly --declarationMap false --declaration --declarationDir dist/types",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "lint": "eslint \"src/**/*.ts*\"",
    "test": "jest --passWithNoTests",
    "manual-release": "pnpm run build && pnpm publish --no-git-checks"
  },
  "sideEffects": false,
  "keywords": [
    "react",
    "sdui",
    "server-driven-ui",
    "components",
    "radix-ui",
    "ui-library",
    "typescript"
  ],
  "peerDependencies": {
    "react": "^18.0",
    "react-dom": "^18.0",
    "@lodado/sdui-template": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.2.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "babel-jest": "29.5.0",
    "eslint": "^8.57.0",
    "eslint-config-sdui-template": "workspace:*",
    "jest-config": "workspace:*",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup-config": "workspace:*",
    "ts-jest": "29.0.5",
    "tsconfig": "workspace:*",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lodado/sdui-template.git",
    "directory": "apps/sdui-template-component"
  },
  "homepage": "https://github.com/lodado/sdui-template#readme",
  "bugs": {
    "url": "https://github.com/lodado/sdui-template/issues"
  }
}

